                BorderThickness="5"
                CornerRadius="8,8,0,8" Margin="10,0,0,0" >
        </Border>

        <!--// Menu Border //-->
        <Border Height="40"
                Width="935"
                Background="#363435"
                BorderBrush="#363435"
                BorderThickness="3"
                VerticalAlignment="Top"
                HorizontalAlignment="Center"
                Margin="0,35,0,0"
                CornerRadius="5" />

        <!--// Menu //-->
        <Menu Height="30"
              Width="890"
